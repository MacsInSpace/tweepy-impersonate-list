

def destroyListIfRequired(api,tag):
  lists=api.lists_all()
  listexists= False

  for l in lists:
    for l2 in l:
      if type(l2) is tweepy.models.List:
        if l2.slug==tag:
          listexists=True
          report("List appears to exist...destroying it now")
          api.destroy_list(l2.slug)

  if listexists is False:
    report("List did not appear to exist...")









